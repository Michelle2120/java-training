package Tutorials;

public class DoWhile {
public static void main(String[] args) {
	String[] familymembers = new String[5];
	familymembers [0]="Anesu";
	familymembers [1]="Tapiwa";
	familymembers [2]="Sophie";
	familymembers [3]="Faith";
	familymembers [4]="Kuda";
	int n = 0;
	do{
		System.out.println("Their name is" + familymembers[0]);
	}
		while( n<=2) {
	System.out.println("Hello" + familymembers);
    }
}
  }
