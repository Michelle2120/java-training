package Tutorials;

public class SimpleProject2 {

	public static void main(String[] args) {
		// Write a simple project using if/else statements
		
		int freeHours = 8;
		if(freeHours < 8){
			System.out.println("You are wasting time");
		}
		else if(freeHours > 8){
			System.out.println("You are on the right track");
		}

		else {
			System.out.println("Get your life in order");
		  }
		}
	
