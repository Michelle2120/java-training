package Assignment;

public class Lab1 {

	public static void main(String[] args) {
		// This function will take a value n and return the sum of numbers 1 to n
		int n;
		int sum = 0;
    public static sum(){
    	for(int i = 1; i<=n; i++){
    		sum = sum + 1;
    		System.out.println(sum (4));
    	}
    	return sum;
    }
	 }
}	
	


